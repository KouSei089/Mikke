<%= form_with model: @user, url: password_reset_path(@token), local: true do |f| %>
  <%= f.label :email %><br>
  <%= @user.email %>

  <%= f.label :password %><br>
  <%= f.password_field :password %>

  <%= f.label :password_confirmation %><br>
  <%= f.password_field :password_confirmation %>

  <%= f.submit %>
<% end %>
